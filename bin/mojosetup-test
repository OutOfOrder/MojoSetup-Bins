#!/bin/sh

SETUPDIR="$1"

if [ -z "$SETUPDIR" ]; then
    echo "Usage $0 Application.mojo"
    exit 1
fi


MOJO_DIR=`dirname $0`/mojosetup/
if [ `arch` == "x86_64" ]; then
    MOJO_BINDIR=$MOJO_DIR/x86_64
else
    MOJO_BINDIR=$MOJO_DIR/x86
fi

export MOJOSETUP_GUIPATH=$MOJO_BINDIR
export MOJOSETUP_BASE=$SETUPDIR
$MOJO_BINDIR/mojosetup --log=- "$@"

